{
  "name": "fact-service",
  "version": "1.1.1",
  "description": "Enhanced Key-Value lookup service. Uses Nodejs, TypeScript, Express & Knex Project.",
  "main": "index.js",
  "bin": {
    "fact-cli": "./bin/fact-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justsml/fact-service.git"
  },
  "homepage": "https://github.com/justsml/fact-service",
  "scripts": {
    "start": "nodemon --watch './**/*.ts' --exec 'ts-node' index.ts",
    "test": "DB_ADAPTER=postgres vitest",
    "test:pg": "DB_ADAPTER=postgres vitest",
    "test:redis": "DB_ADAPTER=redis vitest",
    "test:dynamo": "DB_ADAPTER=dynamo vitest",
    "test:cassandra": "DB_ADAPTER=cassandra vitest",

    "format": "prettier --write './**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
    "cli:get": "./bin/fact-cli get",
    "cli:set": "./bin/fact-cli set",
    "cli:del": "./bin/fact-cli del",
    "cli:list": "./bin/fact-cli list"
  },
  "keywords": [
    "key-value",
    "postgres",
    "lookup",
    "service",
    "nodejs",
    "typescript",
    "express",
    "knex"
  ],
  "author": {
    "name": "Dan Levy",
    "url": "https://danlevy.net"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0",
    "yarn": ">=1.22.0"
  },
  "engineStrict": true,
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.478.0",
    "@aws-sdk/lib-dynamodb": "^3.478.0",
    "@elite-libs/auto-config": "^1.4.1",
    "axios": "^1.6.2",
    "cassandra-driver": "^4.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pino": "^8.17.1",
    "pino-http": "^8.6.0",
    "pino-pretty": "^10.3.0",
    "ulidx": "^2.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.189",
    "@types/morgan": "^1.9.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.8",
    "@types/supertest": "^2.0.16",
    "ms": "^2.1.3",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
